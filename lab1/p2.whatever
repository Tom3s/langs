var a: int = read().toInt()


func isPrime(n: int) -> bool {
	if n <= 1 {
		return false
	}

	if n == 2 {
		return true
	}

	if n % 2 == 0 {
		return false
	}

	for (var i: int = 0) (i <= sqrt(n)) (i++) {
		if n % i == 0 {
			return false
		}
	}

	return true
}

print(a, " is ", isPrime(a) ? "" : "not ", "prime")
